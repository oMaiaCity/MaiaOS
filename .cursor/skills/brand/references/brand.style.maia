{
  "$schema": "@schema/style",
  "$id": "@brand/style/brand",
  "tokens": {
    "colors": {
      "marineBlue": "#001F33",
      "marineBlueMuted": "#2D4A5C",
      "marineBlueLight": "#5E7A8C",
      "paradiseWater": "#00BDD6",
      "lushGreen": "#4E9A58",
      "terracotta": "#C27B66",
      "sunYellow": "#E6B94D",
      "softClay": "#E8E1D9",
      "tintedWhite": "#F0EDE6",
      "background": "transparent",
      "foreground": "#F0EDE6",
      "primary": "#00BDD6",
      "secondary": "#E8E1D9",
      "border": "rgba(255, 255, 255, 0.1)",
      "interactive": "#00BDD6",
      "surface": "rgba(255, 255, 255, 0.0005)",
      "overlay": "rgba(0, 0, 0, 0.40)",
      "text": {
        "marine": "#D1E8F7",
        "water": "#004D59",
        "green": "#F0F9F1",
        "terracotta": "#FDF2EF",
        "yellow": "#4D3810"
      }
    },
    "spacing": {
      "xs": "0.5rem",
      "sm": "0.75rem",
      "md": "1rem",
      "lg": "1.5rem",
      "xl": "2rem",
      "2xl": "3rem",
      "3xl": "4rem",
      "hero": "10vh"
    },
    "typography": {
      "fontFamily": {
        "heading": "'Alice', serif",
        "body": "'Plus Jakarta Sans', sans-serif"
      },
      "fontFaces": [
        {
          "fontFamily": "Alice",
          "src": "url('/brand/fonts/Alice/Alice-Regular.ttf') format('truetype')",
          "fontWeight": "400",
          "fontStyle": "normal",
          "fontDisplay": "swap"
        },
        {
          "fontFamily": "Plus Jakarta Sans",
          "src": "url('/brand/fonts/Jarkata/PlusJakartaSans-VariableFont_wght.ttf') format('truetype')",
          "fontWeight": "100 900",
          "fontStyle": "normal",
          "fontDisplay": "swap"
        },
        {
          "fontFamily": "Plus Jakarta Sans",
          "src": "url('/brand/fonts/Jarkata/PlusJakartaSans-Italic-VariableFont_wght.ttf') format('truetype')",
          "fontWeight": "100 900",
          "fontStyle": "italic",
          "fontDisplay": "swap"
        }
      ],
      "fontSize": {
        "xs": "0.75rem",
        "sm": "0.85rem",
        "base": "1rem",
        "lg": "1.15rem",
        "xl": "1.5rem",
        "2xl": "2rem",
        "3xl": "3rem",
        "h1": "clamp(3.5rem, 12vw, 8rem)",
        "h2": "clamp(2rem, 5vw, 4rem)",
        "h3": "clamp(1.5rem, 4vw, 2.5rem)",
        "subheading": "clamp(1.5rem, 4vw, 2.5rem)"
      },
      "fontWeight": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700"
      },
      "lineHeight": {
        "tight": "0.9",
        "normal": "1.5",
        "relaxed": "1.6"
      },
      "letterSpacing": {
        "normal": "0",
        "tight": "-0.02em",
        "wide": "0.05em",
        "wider": "0.1em",
        "widest": "0.2em"
      },
      "shadows": {
        "textGlow": "0 0 30px rgba(232, 225, 217, 0.2)",
        "textGlowStrong": "0 0 30px rgba(240, 237, 230, 0.3)"
      }
    },
    "radii": {
      "sm": "4px",
      "md": "12px",
      "apple": "18px",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0 4px 30px rgba(0, 0, 0, 0.05)",
      "md": "0 10px 30px rgba(0, 0, 0, 0.05)",
      "lg": "0 10px 40px rgba(0, 0, 0, 0.15)",
      "glassGlow": "inset 0 1px 1px 0 rgba(255, 255, 255, 0.05)"
    },
    "effects": {
      "liquidMembrane": {
        "background": "rgba(255, 255, 255, 0.0005)",
        "backdropFilter": "blur(8px) saturate(150%) contrast(101%)",
        "border": "1px solid rgba(255, 255, 255, 0.05)"
      },
      "underwritingGlass": {
        "background": "rgba(255, 255, 255, 0.15)",
        "backdropFilter": "blur(12px) saturate(140%)"
      },
      "underwritingSubtle": {
        "background": "rgba(255, 255, 255, 0.02)",
        "backdropFilter": "blur(4px) saturate(120%)"
      },
      "navNotch": {
        "background": "rgba(232, 225, 217, 0.75)",
        "backdropFilter": "blur(12px) saturate(120%)",
        "border": "1px solid rgba(255, 255, 255, 0.4)"
      }
    },
    "transitions": {
      "fast": "all 0.3s cubic-bezier(0.4, 0, 0.2, 1)",
      "standard": "all 0.5s cubic-bezier(0.2, 0, 0.2, 1)",
      "slow": "all 0.7s ease-out"
    }
  },
  "components": {
    "btn": {
      "display": "inline-flex",
      "alignItems": "center",
      "justifyContent": "center",
      "padding": "0.9rem 2.5rem",
      "borderRadius": "{radii.full}",
      "fontFamily": "{typography.fontFamily.body}",
      "fontWeight": "{typography.fontWeight.semibold}",
      "fontSize": "{typography.fontSize.sm}",
      "letterSpacing": "{typography.letterSpacing.wide}",
      "textTransform": "uppercase",
      "cursor": "pointer",
      "transition": "{transitions.fast}",
      "border": "1.5px solid transparent",
      "@container (max-width: 400px)": {
        "padding": "0.6rem 1.5rem",
        "fontSize": "0.75rem"
      },
      ":hover": {
        "transform": "translateY(-2px)",
        "filter": "brightness(1.1)",
        "boxShadow": "0 10px 30px rgba(0,0,0,0.1)"
      },
      ":active": {
        "transform": "scale(0.97)"
      }
    },
    "btnSolidMarine": {
      "extends": "btn",
      "background": "{colors.marineBlue}",
      "color": "{colors.text.marine}",
      "borderColor": "rgba(255,255,255,0.1)"
    },
    "btnSolidWater": {
      "extends": "btn",
      "background": "{colors.paradiseWater}",
      "color": "{colors.text.water}",
      "borderColor": "rgba(255,255,255,0.2)"
    },
    "btnOutlineMarine": {
      "extends": "btn",
      "background": "transparent",
      "color": "{colors.marineBlue}",
      "borderColor": "{colors.marineBlue}"
    },
    "btnGlassWater": {
      "extends": "btn",
      "background": "rgba(0, 189, 214, 0.2)",
      "backdropFilter": "blur(8px)",
      "borderColor": "rgba(0, 189, 214, 0.3)",
      "color": "{colors.paradiseWater}",
      "boxShadow": "inset 0 1px 1px rgba(255, 255, 255, 0.2)"
    },
    "card": {
      "containerType": "inline-size",
      "borderRadius": "{radii.apple}",
      "overflow": "hidden",
      "transition": "{transitions.standard}"
    },
    "liquidMembrane": {
      "extends": "card",
      "background": "{effects.liquidMembrane.background}",
      "backdropFilter": "{effects.liquidMembrane.backdropFilter}",
      "border": "{effects.liquidMembrane.border}",
      "padding": "{spacing.2xl}",
      "boxShadow": "{shadows.lg}",
      "position": "relative",
      "@container (max-width: 600px)": {
        "padding": "{spacing.xl}"
      }
    },
    "navNotch": {
      "position": "fixed",
      "top": "2rem",
      "left": "50%",
      "transform": "translateX(-50%)",
      "zIndex": "1000",
      "display": "flex",
      "alignItems": "center",
      "padding": "0.6rem 1.2rem",
      "background": "{effects.navNotch.background}",
      "backdropFilter": "{effects.navNotch.backdropFilter}",
      "border": "{effects.navNotch.border}",
      "borderRadius": "{radii.full}",
      "boxShadow": "{shadows.lg}",
      "width": "fit-content"
    },
    "container": {
      "containerType": "inline-size",
      "maxWidth": "80rem",
      "marginLeft": "auto",
      "marginRight": "auto",
      "paddingLeft": "{spacing.md}",
      "paddingRight": "{spacing.md}",
      "@container (min-width: 768px)": {
        "paddingLeft": "{spacing.xl}",
        "paddingRight": "{spacing.xl}"
      }
    },
    "grid": {
      "display": "grid",
      "gridTemplateColumns": "1fr",
      "gap": "{spacing.xl}",
      "@container (min-width: 700px)": {
        "gridTemplateColumns": "repeat(3, 1fr)",
        "gap": "{spacing.2xl}"
      }
    }
  },
  "selectors": {
    ":host": {
      "position": "relative",
      "minHeight": "100vh",
      "fontFamily": "{typography.fontFamily.body}",
      "color": "{colors.foreground}",
      "background": "#000"
    },
    ":host::before": {
      "content": "\"\"",
      "position": "fixed",
      "inset": "0",
      "zIndex": "-1",
      "backgroundImage": "url('$backgroundImage')",
      "backgroundSize": "cover",
      "backgroundPosition": "center",
      "backgroundAttachment": "fixed"
    },
    ":host::after": {
      "content": "\"\"",
      "position": "fixed",
      "inset": "0",
      "zIndex": "0",
      "background": "{colors.overlay}"
    }
  }
}

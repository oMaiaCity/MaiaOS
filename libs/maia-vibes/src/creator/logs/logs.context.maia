{
	"$schema": "°Maia/schema/context",
	"$id": "°Maia/creator/context/logs",
	"messages": {
		"schema": "°Maia/schema/message",
		"map": {
			"id": "id",
			"type": "type",
			"processed": "processed",
			"payload": "payload",

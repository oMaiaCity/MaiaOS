{
	"$schema": "°Maia/schema/meta",
	"$id": "°Maia/schema/data/human",
	"title": "°Maia/schema/data/human",
	"description": "Virtual schema for Human Book view. Shape: id, accountId, registryName, profileName. Data from account.registries.humans.",
	"cotype": "comap",
	"indexing": false,
	"properties": {
		"id": {
			"type": "string",
			"pattern": "^co_z[a-zA-Z0-9]+$",
			"description": "Human CoMap co-id"
		},
		"accountId": {
			"type": "string",
			"pattern": "^co_z[a-zA-Z0-9]+$",
			"description": "Account co-id"
		},
		"registryName": {
			"type": "string",
			"description": "Registry key (animal-generated, e.g. brave-dolphin-71234567)"
		},
		"profileName": {
			"type": "string",
			"description": "Profile display name from profile.name"
		}
	},
	"required": ["id", "accountId", "registryName", "profileName"]
}

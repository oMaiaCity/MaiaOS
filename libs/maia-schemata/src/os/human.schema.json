{
	"$schema": "°Maia/schema/meta",
	"$id": "°Maia/schema/os/human",
	"title": "°Maia/schema/os/human",
	"description": "Canonical public identity: account + profile. Referenced by humans registry.",
	"cotype": "comap",
	"indexing": false,
	"properties": {
		"account": {
			"type": "string",
			"pattern": "^co_z[a-zA-Z0-9]{20,}$",
			"description": "Account co-id"
		},
		"profile": {
			"type": "string",
			"pattern": "^co_z[a-zA-Z0-9]{20,}$",
			"description": "Profile co-id"
		}
	},
	"required": ["account", "profile"],
	"additionalProperties": false
}

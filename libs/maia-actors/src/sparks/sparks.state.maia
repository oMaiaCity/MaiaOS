{
	"$schema": "°Maia/schema/state",
	"$id": "°Maia/actor/sparks/state",
	"initial": "idle",
	"states": {
		"idle": {
			"on": {
				"SPARK_OP": {
					"target": "processing"
				}
			}
		},
		"processing": {
			"entry": [
				{
					"function": true
				}
			],
			"on": {
				"SUCCESS": {
					"target": "idle"
				},
				"ERROR": {
					"target": "idle"
				}
			}
		}
	}
}

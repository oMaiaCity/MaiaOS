{
	"$schema": "°Maia/schema/process",
	"$id": "°Maia/actor/views/actor-list/process",
	"@label": "@maia/actor/views/actor-list/process",
	"handlers": {
		"SELECT_ACTOR": [
			{
				"ctx": {
					"selectedActor": {
						"id": "$$id",
						"label": "$$label",
						"interface": "$$interface",
						"interfaceItems": {
							"$map": {
								"array": "$$interface",
								"as": "item",
								"return": {
									"name": "$$"
								}
							}
						}
					}
				}
			}
		]
	}
}

{
	"$schema": "°Maia/schema/process",
	"$id": "°Maia/actor/services/schema-ops/process",
	"@label": "@maia/actor/services/schema-ops/process",
	"handlers": {
		"READ_SCHEMA": [
			{
				"op": {
					"readSchema": {
						"schemaRef": "$$schemaRef"
					}
				}
			},
			{
				"tell": {
					"target": "$$source",
					"type": "SUCCESS",
					"payload": {
						"definition": "$$result.definition"
					}
				}
			}
		],
		"SUCCESS": [
			{
				"ctx": {}
			}
		],
		"ERROR": [
			{
				"ctx": {
					"error": "$$errors.0.message"
				}
			}
		]
	}
}
